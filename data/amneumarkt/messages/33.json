                        "text": {
                            "_": "TextPlain",
                            "text": "Disney will lay off 28,000 workers as the pandemic continues to ravage its theme park business"
                        }
                    },
                    {
                        "_": "PageBlockAuthorDate",
                        "author": {
                            "_": "TextUrl",
                            "text": {
                                "_": "TextPlain",
